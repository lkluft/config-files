# ssh configuration file
# settings for university servers
Host login
    Hostname login.zmaw.de
    User u300509

Host t7
    Hostname thunder7
    User u300509
    ProxyCommand ssh -q login nc -q0 %h 22
    LocalForward 4227 localhost:4227
    LocalForward 4234 localhost:4234
    ForwardX11 yes
    ForwardX11trusted yes

Host apple
    Hostname apple157.mi.zmaw.de
    User u300509
    ProxyCommand ssh -q login nc -q0 %h 22

Host lehre
    Hostname lehre3
    User u300509
    ProxyCommand ssh -q login nc -q0 %h 22

Host cluster
    Hostname cluster.wr.informatik.uni-hamburg.de
    User kluft
    ForwardX11 yes
    ForwardX11trusted yes

# Host uni_info
#     Hostname  rzssh1.informatik.uni-hamburg.de
