# ssh configuration file
ForwardAgent yes
ForwardX11 yes
LogLevel ERROR

# settings for university servers
Host login1
    Hostname login1.zmaw.de
    User u300509

Host thun
    Hostname thunder7
    User u300509
    LocalForward 4242 localhost:4242
    ProxyCommand ssh -q login1 nc -q0 thunder7 22

Host lehre
    Hostname lehre2
    User u300509
    LocalForward 4242 localhost:4242
    ProxyCommand ssh -q login1 nc -q0 lehre2 22

