# ssh configuration file
# settings for university servers
Host login
    Hostname login2.zmaw.de
    User u300509

Host thun
    Hostname thunder7
    User u300509
    LocalForward 4242 localhost:4242
    ProxyCommand ssh -q login nc -q0 %h 22
    ForwardX11 yes
    ForwardX11trusted yes

Host apple
    Hostname apple157.mi.zmaw.de
    User u300509
    LocalForward 4242 localhost:4242
    ProxyCommand ssh -q login nc -q0 %h 22

Host cluster
    Hostname cluster.wr.informatik.uni-hamburg.de
    User kluft
    ForwardX11 yes
    ForwardX11trusted yes

Host lehre
    Hostname lehre1
    User u300509
    LocalForward 4242 localhost:4242
    ProxyCommand ssh -q login nc -q0 %h 22

